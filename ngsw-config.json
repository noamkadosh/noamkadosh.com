{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://s3-us-west-1.amazonaws.com/www.noamkadosh.com/favicon.ico",
          "https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500&amp;display=swap",
          "https://fonts.googleapis.com/icon?family=Material+Icons",
          "https://fonts.gstatic.com/**"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": [
        "https://api.noamkadosh.com/twitter",
        "https://api.noamkadosh.com/github",
        "https://pbs.twimg.com/**"
      ],
      "cacheConfig": {
        "maxSize": 10,
        "maxAge": "1d",
        "strategy": "performance"
      }
    }
  ]
}
